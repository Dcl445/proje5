


  Scanner klv=new Scanner(System.in);
        
        System.out.println("lutfen sayi giriniz");
             int sayi=klv.nextInt();
        
        for (int i = 1; i <sayi; i*=4) {
            System.out.print(i+",");
                
            }
            
          for (int j = 1; j <sayi; j*=5) {
              System.out.print(j+",");
          }
        System.out.println(" ");
        
        
